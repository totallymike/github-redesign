{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "error",
  "jsRules": {},
  "linterOptions": {
    "exclude": ["src/generated/*"]
  },
  "rules": {
    "prettier": true,
    "array-bracket-spacing": false,
    "arrow-parens": true,
    "ter-arrow-spacing": true,
    "ban": [true, ["alert"]],
    "block-spacing": true,
    "brace-style": [true, "1tbs", {"allowSingleLine": true}],
    "trailing-comma": [true, "never"],
    "ter-computed-property-spacing": true,
    "curly": true,
    "eofline": true,
    "triple-equals": [true, "allow-null"],
    "forin": true,
    "interface-name": false,
    "ter-indent": [true, 2, {"SwitchCase": 1}],
    "linebreak-style": true,
    "member-access": false,
    "newline-per-chained-call": false,
    "new-parens": true,
    "no-arg": true,
    "no-console": true,
    "no-constant-condition": true,
    "no-debugger": true,
    "no-duplicate-case": true,
    "no-duplicate-imports": true,
    "no-empty": true,
    "no-eval": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-extra-semi": true,
    "no-switch-case-fall-through": true,
    "no-inner-declarations": true,
    "no-invalid-regexp": true,
    "no-irregular-whitespace": true,
    "ter-no-mixed-spaces-and-tabs": true,
    "no-multi-spaces": [true, {"ignoreEOLComments": true}],
    "no-multiline-string": false,
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-octal-literal": true,
    "ter-no-proto": true,
    "no-duplicate-variable": true,
    "no-regex-spaces": true,
    "ter-no-script-url": true,
    "ter-no-self-compare": true,
    "no-shadowed-variable": true,
    "ter-func-call-spacing": [true, "never"],
    "no-sparse-arrays": true,
    "no-string-throw": true,
    "no-trailing-whitespace": true,
    "no-unexpected-multiline": true,
    "no-unsafe-finally": true,
    "no-var-keyword": true,
    "no-with-statement": true,
    "number-literal-format": true,
    "object-curly-spacing": [true, "always"],
    "one-variable-per-declaration": true,
    "prefer-const": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "quotemark": [true, "single", "jsx-double"],
    "radix": true,
    "semicolon": [true, "always"],
    "space-before-function-paren": [true, {"named": "never"}],
    "space-in-parens": true,
    "comment-format": true,
    "use-isnan": true,
    "valid-typeof": true,
    "binary-expression-operand-order": true,
    "only-arrow-functions": false,
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "jsx-no-lambda": false,
    "ordered-imports": [true, {
      "named-imports-order": "lowercase-last"
    }]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules",
    "node_modules/tslint-microsoft-contrib"
  ]
}
